@import '../stopFee/stopFee.wxss';
view{font-size:14px;}
.store-info{padding:30rpx;box-sizing: border-box;}
.store-title{font-size:32rpx;text-align: center;margin-bottom:96rpx;}
.input-store-info{width:510rpx;margin:0 auto;}
.column-info text{color:#666;font-size:34rpx;display:block;margin-bottom:18rpx;}
.column-info .store-name{border:1px solid #ccc;width:100%;height:100rpx;line-height:84rpx;font-size:28rpx;padding:0 10px;box-sizing: border-box;}
.column-info textarea{
  border:1px solid #ccc;width:100%;max-height:100rpx;font-size:28rpx;padding:8px 10px;box-sizing: border-box;
}
.txt-num{text-align:right;font-size:24rpx;color:#888;line-height:1.6;}
.choice-title{float:left;width:50%;text-align:center;font-size:28rpx;}






